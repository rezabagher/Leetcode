class Solution(object):
    def moveZeroes(self, nums):
        i = 0 # for zero pointers
        j = 0 # for non-zero pointers

        while i < len(nums) and j < len(nums) : # continue till end of the list , i and j both should be checked in case all are non zero we need to check i and for all zero case we need to check j
                    if nums[i]  == 0:
                        if nums[j] != 0 and i < j :
                             nums[i], nums[j] = nums[j], nums[i] # changing two values
                             i = i + 1 # i would be non zero so we move forward 
                        j = j + 1 # going to next non zero value
                    else:
                     i = i + 1 # if the value is zero , we won't change anything and move on to the next value
        return nums
